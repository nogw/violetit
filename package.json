{
  "name": "violetit",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --maxWorkers=25%",
    "test:server": "yarn workspace @violetit/server test",
    "copy-envs": "turbo run copy-env",
    "dev:all": "turbo run start:dev --parallel",
    "dev:web": "yarn workspace @violetit/web start:dev",
    "dev:server": "yarn workspace @violetit/server start:dev"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-relay": "^1.8.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "turbo": "^1.8.5",
    "typescript": "^4.9.4"
  }
}
